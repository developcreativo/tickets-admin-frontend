{
    "name": "@tickets-admin/ui-components",
    "version": "2.0.0",
    "description": "Librería de componentes Vue 3 reutilizables para el sistema de administración de tickets",
    "type": "module",
    "main": "./dist/tickets-ui-components.umd.js",
    "module": "./dist/tickets-ui-components.es.js",
    "exports": {
        ".": {
            "import": "./dist/tickets-ui-components.es.js",
            "require": "./dist/tickets-ui-components.umd.js"
        },
        "./style": "./dist/tickets-ui-components.css"
    },
    "files": [
        "dist",
        "src",
        "README.md"
    ],
    "scripts": {
        "build:lib": "vite build --config build-lib.config.js",
        "dev:lib": "vite --config build-lib.config.js",
        "preview:lib": "vite preview --port 3001",
        "clean": "rimraf dist",
        "prebuild": "npm run clean"
    },
    "keywords": [
        "vue",
        "vue3",
        "components",
        "ui",
        "library",
        "tickets",
        "admin",
        "dashboard",
        "form",
        "table",
        "modal",
        "button",
        "input",
        "select"
    ],
    "author": "Equipo de Desarrollo - Tickets Admin",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/tickets-admin/ui-components.git"
    },
    "bugs": {
        "url": "https://github.com/tickets-admin/ui-components/issues"
    },
    "homepage": "https://github.com/tickets-admin/ui-components#readme",
    "peerDependencies": {
        "vue": "^3.4.0",
        "vue-router": "^4.2.0",
        "pinia": "^2.1.0"
    },
    "devDependencies": {
        "@vitejs/plugin-vue": "^5.0.0",
        "vite": "^5.0.0",
        "terser": "^5.0.0",
        "rimraf": "^5.0.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ]
}